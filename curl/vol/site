        <script type="text/javascript">
    function passwordResetSubmit(){
        var loginEmail = $("#start-signup-email");
        var warningDiv = $("#warning-box");
        var successDiv = $("#form-success");

        var data = {
            csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
            email: loginEmail.val()
        };
        var missingFields = [];
        if (!loginEmail.val()){
            missingFields.push("<li><strong>Email</strong> é um campo obrigatório</li>");
        }
        if (missingFields.length > 0) {
            if (missingFields.length > 1){
                warningDiv.html("Corrija os seguintes erros:<ul>"+missingFields.join("")+"</ul>");
            } else {
                warningDiv.html("Corrija o seguinte erro:<ul>"+missingFields.join("")+"</ul>");
            }
            warningDiv.slideDown("fast");
            return false;
        }

        $.ajax({
            url: "/password/recover",
            type: "POST",
            data: data

        }).done(function(response){
        }).success(function(response){
            if (response && response.success){
                $("#forgot-password").html("<ul class='mdl-color--yellow-50' style='margin: 0 auto; list-style-type: none; padding:10px;'><li>"+response.message+"</li></ul>");
                $("#forgot-password").css("display", "contents");
            } else {
                warningDiv.html("Corrija os seguinte erro:<ul><li>"+response.message+"</li></ul>");
                warningDiv.slideDown("fast");
            }
        }).fail(function(response){
            warningDiv.html("Corrija os seguinte erro:<ul><li>"+response.responseJSON.message+"</li></ul>");
            warningDiv.slideDown("fast");
        });

    };

