function mixpanel_success_action(action_name, action_data, user_mixpanel_info){
    data = user_mixpanel_info;
    if(action_data){
        if(action_data.form){
            action_data = action_data.form;
        }
        if(action_data.csrf_token){
            delete action_data.csrf_token;
        }
        if(action_data.location){
            action_data = Object.assign(action_data, action_data.location);
            delete action_data.location;
        }
        data = Object.assign(data, action_data);
    }
    mixpanelJS.mixpanelSendEvent(action_name, data);
};

