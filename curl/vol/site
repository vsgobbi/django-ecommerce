                $.ajax({
                    url: "/dashboard/process_payment",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify(data)
                }).done(fn)
                .fail(function(response){
                    let messageBackDrop = "Verifique as informações fornecidas e tente novamente.<br>:NEW_MSG:Caso o problema persista entre contato conosco através<br>do e-mail <a href='mailto:partyou@partyou.com.br'>partyou@partyou.com.br</a>";
                    const respJson      = response.responseJSON;

                    if(data.showMessage !== undefined && respJson.message !== undefined){
                        messageBackDrop = messageBackDrop.replace(':NEW_MSG:', 
                            "<ul><li>"+respJson.message+"</li></ul>"
                        );
                    } else {
                        messageBackDrop = messageBackDrop.replace(':NEW_MSG:', '');
                    }

                    modalJS.showLoadingBackdrop("error",
                    title="Compra não aprovada!",
                    message=messageBackDrop,
                    showLoadingBar=false,
                    showCloseButton=true,
                    reloadPageOnClose=true,
                    closeModalOnClose=false);
                });
            },
            addItemToUserCart: function(e, itemId, itemQtyToAdd){
                let url = "/user/add-share-item-to-cart";
                let mixpanelMessage = "Part Add To Cart";
                userJS.shoppingCart.updateUserCart(e, url, mixpanelMessage);
