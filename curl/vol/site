            resetCredentials: function(keepEmail){
                if(!keepEmail){
                    $("#start-signup-email").val("");
                    $("#start-signup-email").parent().removeClass("is-dirty");
                    $("#start-signup-email").parent().removeClass("is-invalid");
                }

                $(".signup-field").hide("fast");
                $(".login-field").hide("fast");

                $(".signup-field input").each(function(){
                    $(this).parent().removeClass("is-dirty");
                    $(this).val("");
                });
                $(".login-field input").each(function(){
                    $(this).parent().removeClass("is-dirty");
                    $(this).val("");
                });

                $("#submit-email-signup-button").hide();
                $("#submit-email-login-button").hide();
                $("#start-email-login-button").show();
                $("#forgot-password").hide();

                warningBoxJS.displayError();
            },
            checkCredentials: function(){
                userEmail = $("#start-signup-email");

                let missingFields = []
                if(
                    userEmail.val() &&
                    (!userJS.validEmail(userEmail.val()) ||
                    // !userEmail.val() ||
                    userEmail.parent('.is-invalid').length > 0)
                ){
                    missingFields.push("<li>Utilize um <strong>email</strong> válido</li>");
                }

                if(warningBoxJS.displayError(missingFields) || !userEmail.val()){
                    return false;
                }

                // TODO 403 here
                $.ajax({
                    url: '/users/credentials',
                    method: "POST",
                    data: {
                        csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
                        user_email:userEmail.val()
                    }
                }).done(function(response){
                    $(missingFields).slideUp("fast");
                    $(".signup-field").hide();
                    $(".login-field").hide();

                }).success(function(response){
                    $(this).hide();
                    $("#check-user-credentials").hide();
                    $("#start-email-login-button").hide();
                    if(response.user){
                        $(".login-field").show();
                        $("#signup-login-form").data("user-action", "login");
                        $("#signup-password").focus();
                        $("#submit-email-login-button").show();
                        $("#forgot-password").show();
                    } else {
                        $(".signup-field").show();
                        $("#signup-login-form").data("user-action", "signup");
                        $("#signup-name").focus();
                        $("#submit-email-signup-button").show();
                    }
                    // $('main').animate({
                    //     scrollTop:  $("#session-email-form").offset().top - $('main').children().offset().top
                    // }, 500);
                    // $('#orrsDiag').animate({
                    //     scrollTop:  $("#session-email-form").offset().top - $('#orrsDiag').children().offset().top
                    // }, 500);
                }).error(function(response){
                    $("#check-user-credentials").show();
                    $("#confirm-user-credentials").hide();

                    // TODO FIX AQUI
                    // ajaxErrorResponse(response);
                    ajaxResponseJS.error();
                });
            },
            partnerSessionWithNewEvent: function(e, userAction){
                let url = $("#partner-url").val();
                let confirmationUserEmail = $("#start-signup-email");
                let cpfDocument = $("#document");
                let signupName = $("#signup-name");
                let signupPassword = $("#signup-password");
