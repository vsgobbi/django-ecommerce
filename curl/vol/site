        var keepEmail = true;
        userJS.sessionsJS.resetCredentials(keepEmail);
    });
    $("#reset-session-email").click(function(){
        userJS.sessionsJS.resetCredentials();
        $("#forgot-password").slideUp('fast');
        $("#form-success").hide();
    });
    $("#session-email-button").click(function(){
        var emailInput = $("#start-signup-email");
        $("#session-email-form").slideDown('fast', function() {
            $(emailInput).focus();
        });
        userJS.sessionsJS.checkCredentials();
    });
