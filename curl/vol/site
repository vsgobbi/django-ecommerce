    function sliderInit() {
        $(function() {
            let sliderHtml = $("#slider2");
            $('[data-slider-id="gaxcfu"]').html(sliderHtml);
            $('.slider2').bxSlider({
                "auto":true,
                "mode":"horizontal",
                "pager":true,
                "speed":"1000",
                "controls":false,
                "pagerType":"full",
                "startSlide":0,
                "randomStart":false,
                "slideMargin":0,
                "autoControls":false,
                "touchEnabled": false,
                "swipeThreshold": 50,
                "preventDefaultSwipeY": false,
                "infiniteLoop": true,
                "pause":8000,
                "tickerHover": true
            });
        });
    }
    sliderInit();
