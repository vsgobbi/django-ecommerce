    <script type="text/javascript">
    let valid_zip_code = false;
    // https://www.jqueryscript.net/lightbox/jQuery-Modal-Dialog-Plugin-For-Material-Design-Lite.html
    let eventJS = {
        createPartnerEvent: function(userAction, url){
            let confirmationUserEmail = $("#start-signup-email");
            let cpfDocument = $('#document');
            let signupName = $("#signup-name");
            let signupPassword = $("#signup-password");
            let recaptcha = $("#g-recaptcha-response");

            let adminValue = $("#admin-value-hidden");
            let eventName = $("#event-name");
            let eventNameDefault = $("#default-event-name");
            let guestQty = $("#guest-qty");
            let priceCents = $('#admin-value');
            let eventType = $("#event-type");
            let description = $("#description");

            let warningDiv = $("#form-warning");
            let missingFields = [];

            _data = {
                csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
                signupData: {
                    email: confirmationUserEmail.val(),
                    cpf_document: cpfDocument.val(),
                    name: signupName.val(),
                    password: signupPassword.val(),
                    recaptcha: recaptcha.val(),
                    user_action: userAction
                },
                eventData: {
                    admin_value: adminValue.val(),
                    event_name: eventName.val(),
                    guest_qty: guestQty.val(),
                    price_cents: priceCents.val(),
                    event_type: eventType.val(),
                    description: description.val(),
                    event_name_default: eventNameDefault.val()
                }
            }

            mixpanelJS.mixpanelSuccessAction("Landingpage Chopp Brahma Express User Signup With Email", undefined, {
                csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
                signupData: {
                    email: confirmationUserEmail.val(),
                    cpf_document: cpfDocument.val(),
                    name: signupName.val(),
                    password: signupPassword.val(),
                    recaptcha: recaptcha.val(),
                    user_action: userAction
                },
                eventData: {
                    admin_value: adminValue.val(),
                    event_name: eventName.val(),
                    guest_qty: guestQty.val(),
                    price_cents: priceCents.val(),
                    event_type: eventType.val(),
                    description: description.val(),
                    event_name_default: eventNameDefault.val()
                }
            });

            $.ajax({
                url: url,
                method: "POST",
                data: _data
            }).done(function(response){
                warningDiv.html("");
            }).success(function(response){
                window.open(response.event_url, "_self");
            }).error(function(response){
                if(response && response.messsage){
                    title="Corrija os seguintes erros";
                    message = response.message;
                    warningDiv.html(title+":<ul><li>"+message+"</li></ul>");

                } else if (response && response.responseJSON && response.responseJSON.message) {
                    title="Corrija os seguintes erros";
                    message = response.responseJSON.message
                    warningDiv.html(title+":<ul><li>"+message+"</li></ul>");
                } else {
                    title="Ocorreu um erro interno. Recarregue a página e tente novamente";
                    message = response.responseJSON.message
                    warningDiv.html(title);
                }
                warningDiv.slideDown("fast");
                grecaptcha.reset();
            })
        }
    }
    let shareItemJS = {
        removeImage: function(url, original){
            let formData = new FormData($("#thumb-upload-form")[0]);

            showLoadingBackdrop("waiting",
            title="Imagem",
            message="Estamos removendo a <strong>Imagem</strong>. Por favor, aguarde :)",
            showLoadingBar=true,
            showCloseButton=false,
            reloadPageOnClose=false,
            closeModalOnClose=false);

            $.ajax({
                url: url,
                type: "DELETE",
                data: {
                    thumb_id:$("#thumb_id").val()
                },
                contentType: false,
                cache: false,
                processData: false,
                success: function (response) {
                    $("#thumb_id").val("");
                    $("#image-upload-content").replaceWith(response.html);
                    modalJS.hideLoadingBackdrop();
                },
                error: function(response) {
                    ajaxErrorResponse(response);
                }
            });
        },
        uploadImage: function(url, original){
            let formData = new FormData($("#thumb-upload-form")[0]);

            modalJS.showLoadingBackdrop("waiting",
            title="Imagem",
            message="Estamos fazendo o upload da <strong>Imagem</strong>. Por favor, aguarde :)",
            showLoadingBar=true,
            showCloseButton=false,
            reloadPageOnClose=false,
            closeModalOnClose=false);

            $.ajax({
                url: url,
                type: "POST",
                data: formData,
                contentType: false,
                cache: false,
                processData: false,
                success: function (response) {
                    // if (original) {
                    //     $("#edit-image-picture").attr("src", response.url);
                    //     $("#thumb-id").val(response.img_id);
                    // }
                    // $("#item-thumb").css("background-image", "url("+response.url+")");
                    // $(".thumb-image-id").val(response.img);
                    $("#thumb_id").val(response.thumb_id);
                    $("#image-upload-content").replaceWith(response.html);
                    modalJS.hideLoadingBackdrop();
                    // $("#edit-image-modal").slideUp("fast");
                },
                error: function(response) {
                    ajaxErrorResponse(response);
                    $("#thumb_id").val("");
                }
            });
        },
        animateGoalBar: function(){
            $(".goal-bar.pending").each(function(){
                let goalBar = $(this);
                let totalPriceCentsWithFee = $(goalBar).data("total-price-cents-with-fee");
                let collectedPriceCentsWithFee = $(goalBar).data("collected-price-cents-with-fee");
                let formattedCollectedPriceCentsWithFee = $(goalBar).data("formatted-collected-price-cents-with-fee");
                let textBefore = $(goalBar).data("text-before");
                let textAfter = $(goalBar).data("text-after");
                let partyPopperEmojiId = $(goalBar).data("party-popper-emoji-id");
                let animatePartyPopperEmoji = function(){};
                let removePendingClass = function(elementToRemove){
                    $(elementToRemove).removeClass('pending');
                }

                if (totalPriceCentsWithFee <= collectedPriceCentsWithFee){
                    totalPriceCentsWithFee = 100;
                    collectedPriceCentsWithFee = 100;
                    formattedCollectedPriceCentsWithFee = 100;
                    textBefore = '';
                    textAfter = '% arrecadado';

                    animatePartyPopperEmoji = function(){
                        setTimeout(function(){
                            $(partyPopperEmojiId).fadeIn("fast");
                        }, 1000);
                    }
                }
                // setTimeout(function(){
                    $(goalBar).goalProgress({
                        goalAmount: totalPriceCentsWithFee,
                        currentAmount: collectedPriceCentsWithFee,
                        displayAmount: formattedCollectedPriceCentsWithFee,
                        textBefore: textBefore,
                        textAfter: textAfter
                    }, animatePartyPopperEmoji(), removePendingClass(goalBar));
                // }, 1500);
            })
        },
        config: {
            editDescription: function(){

                let option = "description";
                modalJS.showLoading();
                $.ajax({
                    url: "/share-items/optional",
                    type: 'GET',
                    data: {
                        option: "description"
                    }
                }).done(function(response) {
                    let modalBody = response;
                    let cancelFunction = function(){ }
                    let confirmFunction = function(){
                        $("body #share-item-description").val("SUCESSO");
                    }

                    modalJS.showDialog({
                        title: 'Descrição',
                        text: modalBody,
                        negative: {
                            title: 'cancelar',
                            onClick: cancelFunction
                        },
                        positive: {
                            title: 'confirmar',
                            onClick: confirmFunction
                        },
                        cancelable: false
                    });
                }).fail(function(response) {
                }).always(function(response) {
                    modalJS.hideLoading();
                });
            }
        }
    }
    let modalJS = {
        showLoading: function() {
            // remove existing loaders
            $('.loading-container').remove();
            $('<div id="orrsLoader" class="loading-container" style="position:fixed"><div><div class="mdl-spinner mdl-js-spinner is-active" style="position: absolute;left: 50%;top: 40%;"></div></div></div>').appendTo("body");

            componentHandler.upgradeElements($('.mdl-spinner').get());
            setTimeout(function () {
                $('#orrsLoader').css({opacity: 1});
            }, 1);
        },
        hideLoading: function() {
            $('#orrsLoader').css({opacity: 0});
            setTimeout(function () {
                $('#orrsLoader').remove();
            }, 400);
        },
        showDialog: function(options) {
            options = $.extend({
                id: 'orrsDiag',
                title: null,
                text: null,
                negative: false,
                positive: false,
                cancelable: true,
                contentStyle: null,
                onLoaded: false,
                closeButton: true,
                onclose: null
            }, options);

            const persistData = {
                dialogId: options.id,
                onCloseCallback: options.onclose
            };

            // remove existing dialogs
            $('.dialog-container').remove();
            $(document).unbind("keyup.dialog");

            $('<div id="' + options.id + '" class="dialog-container" style="overflow:scroll;position:fixed"><div class="mdl-card" style="margin: 16px auto 0;padding: 8px 16px; width: 90%;max-width: 1200px"></div></div>').appendTo("body");
            var dialog = $('#orrsDiag');
            var content = dialog.find('.mdl-card');
            if (options.closeButton) {

                const closeFunc = function(){
                    modalJS.hideDialog($("#"+persistData.dialogId));
                    if(persistData.onCloseCallback !== null){
                        persistData.onCloseCallback();
                    }
                }.bind(persistData);

                $('<i id="close-modal" class="material-icons" style="position: absolute; right: 10px; top: 10px; cursor: pointer; z-index: 1">close</i>').appendTo(content);
                $("#close-modal").click(closeFunc);
            }
            if (options.contentStyle != null) content.css(options.contentStyle);
            if (options.title != null) {
                $('<h5>' + options.title + '</h5>').appendTo(content);
            }
            if (options.text != null) {
                $('<p>' + options.text + '</p>').appendTo(content);
            }
            if (options.negative || options.positive) {
                let buttonBar = $('<div class="mdl-card__actions dialog-button-bar"></div>');
                if (options.negative) {
                    options.negative = $.extend({
                        id: 'negative',
                        title: 'Cancel',
                        onClick: function () {
                            return false;
                        }
                    }, options.negative);
                    var negButton = $('<button class="mdl-color-text--grey-400 mdl-button mdl-js-button" id="' + options.negative.id + '"><small>' + options.negative.title + '</small></button>');
                    negButton.click(function (e) {
                        e.preventDefault();
                        if (!options.negative.onClick(e))
                            modalJS.hideDialog(dialog)
                    });
                    negButton.appendTo(buttonBar);
                }
                if (options.positive) {
                    options.positive = $.extend({
                        id: 'positive',
                        title: 'OK',
                        onClick: function () {
                            return false;
                        }
                    }, options.positive);
                    var posButton = $('<button class="mdl-color--light-blue-700 mdl-color-text--white mdl-button mdl-button--colored mdl-js-button" id="' + options.positive.id + '"><small>' + options.positive.title + '</small></button>');
                    posButton.click(function (e) {
                        e.preventDefault();
                        if (!options.positive.onClick(e))
                            modalJS.hideDialog(dialog)
                    });
                    posButton.appendTo(buttonBar);
                }
                buttonBar.appendTo(content);
            }
            componentHandler.upgradeDom();
            if (options.cancelable) {
                dialog.click(function () {
                    modalJS.hideDialog(dialog);
                    if(persistData.onCloseCallback !== null){
                        persistData.onCloseCallback();
                    }
                });
                $(document).bind("keyup.dialog", function (e) {
                    if (e.which == 27)
                        modalJS.hideDialog(dialog);
                        if(persistData.onCloseCallback !== null){
                            persistData.onCloseCallback();
                        }
                });
                content.click(function (e) {
                    e.stopPropagation();
                });
            }
            setTimeout(function () {
                dialog.css({opacity: 1});
                if (options.onLoaded)
                    options.onLoaded();
            }, 1);
        },
        hideDialog: function(dialog) {
            if (!dialog){
                dialog = $("#orrsDiag");
            }
            $(document).unbind("keyup.dialog");
            dialog.css({opacity: 0});
            setTimeout(function () {
                dialog.remove();
            }, 400);
        },
        login: function(){
            let next_uri = $("#next_uri").val();
            modalJS.showLoading();
            $.ajax({
                url: "/users/session/new/form",
                type: 'GET',
                data: {
                    "next_uri":next_uri
                }
            }).done(function() {
                modalJS.hideLoading();
            }).fail(function(response) {
            }).success(function(response) {
                let modalBody = response;
                let cancelFunction = function(){ };
                let confirmFunction = function(){
                    $("body #share-item-description").val("SUCESSO");
                };
                let options = {
                    text: modalBody,
                    cancelable: false
                };
                modalJS.showDialog(options);
                grecaptcha.render("g-recaptcha", {sitekey: "6LfekBwUAAAAAKbScUOZ1BVbO2QGsrkuwm1S9EOX", theme: "light"});
            });
        },
        showLoadingBackdrop: function(backdropType, title, message, showLoadingBar, showCloseButton, reloadPageOnClose, closeModalOnClose, closeCallback){
            $("#backdrop-waiting").hide("fast");
            $("#backdrop-success").hide("fast");
            $("#backdrop-error").hide("fast");
            $("#backdrop-warning").hide("fast");
            $("#close-backdrop").hide("fast");
            $("#reset-backdrop").hide("fast");
            $("#close-modal-backdrop").hide("fast");

            $("#close-backdrop").click(function(){
                if (closeCallback && typeof(closeCallback) === 'function') { closeCallback(); }
                hideLoadingBackdrop();
            });
            $("#reset-backdrop").click(function(){
                window.location.reload(true);
            });
            $("body").on("click","#close-modal-backdrop", function(){
                $("#orrsDiag").hide("fast");
                modalJS.hideLoadingBackdrop();
            });
            $("#backdrop-"+backdropType).show("fast");
            $("#backdrop #backdrop-"+backdropType+" .title").html($.parseHTML(title));
            $("#backdrop #backdrop-"+backdropType+" .message").html($.parseHTML(message));

            if(showLoadingBar){
                $("#backdrop #backdrop-"+backdropType+" .mdl-progress").show("fast");
            } else {
                $("#backdrop #backdrop-"+backdropType+" .mdl-progress").hide("fast");
            }

            if(showCloseButton && reloadPageOnClose){
                $("#close-backdrop").hide("fast");
                $("#reset-backdrop").show("fast");
                $("#close-modal-backdrop").hide("fast");

            } else if (showCloseButton && closeModalOnClose) {
                $("#close-backdrop").hide("fast");
                $("#reset-backdrop").hide("fast");
                $("#close-modal-backdrop").show("fast");

            } else if (showCloseButton) {
                $("#close-backdrop").show("fast");
                $("#reset-backdrop").hide("fast");
                $("#close-modal-backdrop").hide("fast");

            } else {
                $("#close-backdrop").hide("fast");
                $("#reset-backdrop").hide("fast");
                $("#close-modal-backdrop").hide("fast");

            }
            $("#backdrop").fadeIn("fast", function(){});
        },
        hideLoadingBackdrop: function(){
            $("#backdrop").fadeOut('fast', function(){
                $("#backdrop").hide("fast");
                $("#backdrop-waiting").hide("fast");
                $("#backdrop-success").hide("fast");
                $("#backdrop-error").hide("fast");
                $("#backdrop-warning").hide("fast");
                $("#close-backdrop").hide("fast");
                $("#reset-backdrop").hide("fast");
            });
        }
    }
    let navigationJS = {
        windowBack: function(){
            window.history.back();
        }
    }
    let ajaxResponseJS = {
        error: function(){
        },
        upgradeMaterialElements: function(){
            let elementsToUpgrade = $(".must-upgrade");
            elementsToUpgrade.each(function(){
                let classez = $(this).data('material-class');
                $(this).removeClass("must-upgrade");
                $(this).addClass(classez);
            });
            componentHandler.upgradeElements(elementsToUpgrade);
            // }, 0);
        }
    }
    let formatterJS = {
        maskCNPJ: function(elementToMask){
            $(elementToMask).focus(function(){
                $(elementToMask).mask('00000000000000');
            });
            $(elementToMask).blur(function(){
                $(elementToMask).mask('00.000.000/0000-00');
            });
        },
        maskCPF: function(elementToMask){
            $(elementToMask).focus(function(){
                $(elementToMask).mask('00000000000');
            });
            $(elementToMask).blur(function(){
                $(elementToMask).mask('000.000.000-00');
            });
        },
        maskTelephone: function(elementToMask){
            // $(elementToMask).focus(function(){
            //     $(elementToMask).mask('00000000000');
            // });
            // $(elementToMask).blur(function(){
                $(elementToMask).mask('(00) 00000-0000');
            // });

        },
        maskZipCode: function(elementToMask){
            // $(elementToMask).focus(function(){
            //     $(elementToMask).mask('00000000');
            // });
            // $(elementToMask).blur(function(){
                $(elementToMask).mask('00000-000');
            // });

        },
        maskCreditCard: function(elementToMask){
            // $(elementToMask).focus(function(){
            //     $(elementToMask).mask('00000000');
            // });
            // $(elementToMask).blur(function(){
                $(elementToMask).mask('0000000000000000000');
            // });

        },
        maskCVV: function(elementToMask){
            // $(elementToMask).focus(function(){
            //     $(elementToMask).mask('00000000');
            // });
            // $(elementToMask).blur(function(){
                $(elementToMask).mask('00000');
            // });
        },
        maskDayMonthYear: function(elementToMask){
            $(elementToMask).mask('00/00/0000');
        },
        maskCardExpirationDate: function(elementToMask){
            // $(elementToMask).focus(function(){
            //     $(elementToMask).mask('00000000');
            // });
            // $(elementToMask).blur(function(){
                $(elementToMask).mask('00/00');
            // });
        }

    }
    let warningBoxJS = {
        snackbar: function(message, actionHandler, actionText){
            let snackbarContainer = document.querySelector('body #partyou-snackbar');

            if (!actionHandler){
                actionHandler = function(event) {
                    $("#partyou-snackbar").removeClass("mdl-snackbar--active");
                };
            }
            if (!actionText){
                actionText = 'x';
            }
            // showSnackbarButton.addEventListener('click', function() {
                // 'use strict';
                // showSnackbarButton.style.backgroundColor = '#' +
                Math.floor(Math.random() * 0xFFFFFF).toString(16);
                let data = {
                    message: $('<span>'+message+'</span>').text(),
                    timeout: 10000,
                    actionHandler: actionHandler,
                    actionText: actionText
                };
                snackbarContainer.MaterialSnackbar.showSnackbar(data);
                ajaxResponseJS.upgradeMaterialElements();
            // });

        },
        displayError: function(errorMessages){
            let warningBoxJS = $("#warning-box");

            if (!errorMessages || errorMessages.length == 0){
                $("#warning-box").hide("fast").empty();
                return false;
            }

            if (errorMessages.length == 1){
                warningBoxJS.html("Corrija o seguinte erro:<ul>"+errorMessages.join("")+"</ul>");
            } else {
                warningBoxJS.html("Corrija os seguintes erros:<ul>"+errorMessages.join("")+"</ul>");
            }
            warningBoxJS.slideDown("fast");
            return true;
        }
    }
    // 
    let mixpanelJS = {
        mixpanelCreatePeople: function(){
            
        },
        mixpanelSendEvent: function(event_name, eventProperties){
            mixpanel.time_event(event_name);
            mixpanel.track(event_name, eventProperties);
        },
        mixpanelIncrementOpenPage: function(page_name){
            let user_info = mixpanelJS.userMixpanelInfo();
            mixpanelJS.mixpanelSendEvent(page_name, user_info);
        },
        mixpanelIncrementOpenModal: function(modal_name){
            mixpanelJS.mixpanelSendEvent(modal_name, mixpanelJS.userMixpanelInfo() );
            mixpanel.people.increment(modal_name, 1);
        },
        userMixpanelInfo: function(){
            
                return {
                    "User Id": "Session:032627b7-23fb-4425-bbfd-7a9ee2882d79",
                    "User Name": 'Guest',
                }
            
        },
        mixpanelSuccessAction: function(action_name, action_data){
            data = mixpanelJS.userMixpanelInfo();
            if(action_data){
                if(action_data.form){
                    action_data = action_data.form;
                }
                if(action_data.csrf_token){
                    delete action_data.csrf_token;
                }
                if(action_data.location){
                    action_data = Object.assign(action_data, action_data.location);
                    delete action_data.location;
                }
                data = Object.assign(data, action_data);
            }
            mixpanelJS.mixpanelSendEvent(action_name, data);
        }
    }
