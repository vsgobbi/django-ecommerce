            updateUserCart: function(e, url, mixpanelMessage){
                let itemId = $(e.currentTarget).data("item-id");
                let itemQty = $(e.currentTarget).parent().data('item-qty');
                let itemQtyId = $(e.currentTarget).parent().data('item-qty-id');
                let itemOriginalPriceId = $(e.currentTarget).parent().data('item-original-price-id');
                let itemPriceId = $(e.currentTarget).parent().data('item-price-id');
                let itemPriceCents = $(e.currentTarget).parent().data('item-price-cents');

                let data = {
                    csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
                    item: itemId
                };
                $.ajax({
                    url: url,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify(data)
                }).success(function(response){
                    let cart = response.cart;

                    if (cart.items[itemId]){
                        $(e.currentTarget).parent().data('item-qty', cart.items[itemId].item_qty);
                        $(itemQtyId).text(cart.items[itemId].item_qty);
                        $(itemOriginalPriceId).text(cart.items[itemId].formatted_original_item_price_cents_with_credit_card_fee)
                        $(itemPriceId).text(cart.items[itemId].formatted_total_item_price_cents_with_credit_card_fee)

                    } else {
                        $(e.currentTarget).parent().data('item-qty', 0);
                        $(itemQtyId).text(0);
                    }
                    $("body #installments-select").replaceWith(response.installments_select);
                    let selectedOption = $(response.installments_select).find("option:selected");

                    $("#iugu-total").data("clean-val", cart.total_price_cents_with_credit_card_fee/10);
                    $("#iugu-total").val(cart.formatted_total_price_cents_with_credit_card_fee);

                    $(".totalValue").text($(selectedOption).data("total-with-installments"));
                    $(".total-payment-value").text($(selectedOption).data("total-with-installments"));
                    $(".installment-configuration").text($(selectedOption).text());

                    mixpanelJS.mixpanelSuccessAction(mixpanelMessage, data);
                    userJS.refreshShoppingCart();
                    ajaxResponseJS.upgradeMaterialElements();

                }).fail(function(response){
                    let unavailable = $("#"+$(e.currentTarget).data("unavailable-id"));
                    $(unavailable).text(response.responseJSON.message);
                    $(unavailable).show();
                    setTimeout(function(){
                        $(unavailable).hide();
                    }, 2000);
                });
            }
        },
        addressJS: {
            validateAddressFields: function(missingFields){
                let zipCode = $("body").find("#address-zip-code");
                let street = $("body").find("#route").val();
                let country = $("body").find("#country").val();
                let vicinity = $("body").find("#vicinity").val();
                let locality = $("body").find("#locality").val();
                let street_number = $("body").find("#street_number").val();
                let address_complement = $("body").find("#address_complement").val();
                let administrative_area_level_1 = $("body").find("#administrative_area_level_1").val();

                if (zipCode.length > 0 && !zipCode.val()){
                    missingFields.push("<li><strong>CEP</strong> é um campo obrigatório</li>");
                    return missingFields;}
                if (zipCode.length > 0 && zipCode.val() && zipCode.val().length < 9){
                    missingFields.push("<li><strong>CEP</strong> inválido</li>");
                    return missingFields;}
                if (street.length == 0){
                    missingFields.push("<li><strong>Rua</strong> é um campo obrigatório</li>");}
                if (vicinity.length == 0){
                    missingFields.push("<li><strong>Bairro</strong> é um campo obrigatório</li>");}
                if (locality.length == 0){
                    missingFields.push("<li><strong>Cidade</strong> é um campo obrigatório</li>");}
                if (street_number.length == 0){
                    missingFields.push("<li><strong>Número</strong> é um campo obrigatório</li>");}
                if (administrative_area_level_1.length == 0){
                    missingFields.push("<li><strong>Estado</strong> é um campo obrigatório</li>");}
                return missingFields;
            },
            searchAddressByZipCode: function(postalCode){
                if(!postalCode || postalCode.length < 9){
                    $("#address-fields").slideUp("fast", function(){
                        $(this).children().remove();
                    });
                    return false;
                }

                $("#address-zip-code").attr("disabled", true);

                data = {
                    csrf_token: "0e8d972a4d3696c373952c5b606e60769dd8d285d286d3411f7a8dbc24101faf2ae180c1721de5481f8c40959a98aca0c04398e913ade2c00517db9e5437a93c",
                    postal_code: postalCode
                }
                $("#zip-code-search").fadeIn("fast");
                $.ajax({
                    url: "/api/address",
                    type: "POST",
                    data: data
                }).done(function (response) {
                }).success(function (response) {
                    $("#address-zip-code").attr("disabled", false);
                    $("#zip-code-search").fadeOut("fast");
                    warningBoxJS.displayError();
                    valid_zip_code = true;
                    // $("#address-fields").slideUp();
                    $("#address-fields").replaceWith(response);
                    setTimeout(function(){
                        $("#address-fields").slideDown();
                    }, 0);
                    ajaxResponseJS.upgradeMaterialElements();
                    setTimeout(function(){
                        $("#street_number").focus();
                    }, 200)
                }).fail(function (response) {
                    $("#address-zip-code").attr("disabled", false);
                    $("#zip-code-search").fadeOut("fast");
                    valid_zip_code = false;
                    warningBoxJS.displayError(["<li><strong>CEP</strong> inválido</li>"])
                    return false;
                });
                // $(".event-show .description a").click(function(e){
                //     e.preventDefault();
                //     let url = $(e.currentTarget).attr('href');
                //     window.open(url, '_blank');
                // });
            }
        },
        sessionsJS: {
            signup: function(){
                let userEmail = $("#start-signup-email");
                let cpfDocument = $("#document");
                let signupName = $("#signup-name");
                let signupPassword = $("#signup-password");
                let recaptcha = $("#g-recaptcha-response");
                let missingFields = [];

                if(
                    !userJS.validEmail(userEmail.val()) ||
                    !userEmail.val() ||
                    userEmail.parent('.is-invalid').length > 0
                ){
                    missingFields.push("<li>Utilize um email válido</li>");
                }
                if(!signupName.val()){
                    missingFields.push("<li><strong>Nome Completo</strong> é um campo obrigatório</li>");
                }
                if (!cpfDocument.val() || cpfDocument.length > 0 && cpfDocument.val() && !userJS.validCPF(cpfDocument.val())){
                    missingFields.push("<li><strong>CPF</strong> inválido</li>");
                }
                if(!signupPassword.val()){
                    missingFields.push("<li>Campo <strong>senha</strong> é obrigatório</li>");
                }
                if(!recaptcha.val()){
                    missingFields.push("<li>Prove que não é um <strong>robô</strong></li>");
                }
                if(warningBoxJS.displayError(missingFields)){
                    return false;
                }
                $("#signup-login-form").submit();
            },
