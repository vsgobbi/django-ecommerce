        validateCPFDigitsForPurchase: function(elementId){
            if($("#orrsDiag #digit-1").val().length == 1 && $("#orrsDiag #digit-2").val().length == 1 && $("#orrsDiag #digit-3").val().length == 1){
                $("body #"+elementId).attr("disabled", false);
            } else {
                $("body #"+elementId).attr("disabled", true);
            }
        },
        validateCPFDigits: function(elementId){
            if($("#digit-1").val().length == 1 && $("#digit-2").val().length == 1 && $("#digit-3").val().length == 1){
                $("body #"+elementId).attr("disabled", false);
            } else {
                $("body #"+elementId).attr("disabled", true);
            }
        },
        validEmail: function(email){
            const re = /^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()\.,;\s@\"]+\.{0,1})+[^<>()\.,;:\s@\"]{2,})$/;
            return re.test(email);
        },
        validSecondEmail: function(secondEmailInput){
            let secondEmail = secondEmailInput.val();
            if(
                secondEmail != undefined &&
                secondEmail != '' &&
                secondEmailInput.parent('.is-invalid').length != 0 &&
                !this.validEmail(secondEmail)
            ){
                return false;
            } else {
                return true;
            }
        },
        validCPF: function(cpf){
            cpf = cpf.replace(/[^\d]+/g,'');   
            if(cpf == '') return false;
            // Elimina CPFs invalidos conhecidos
            if (cpf.length != 11 ||
                cpf == "00000000000" ||
                cpf == "11111111111" ||
                cpf == "22222222222" ||
                cpf == "33333333333" ||
                cpf == "44444444444" ||
                cpf == "55555555555" ||
                cpf == "66666666666" ||
                cpf == "77777777777" ||
                cpf == "88888888888" ||
                cpf == "99999999999")
                    return false;      
            // Valida 1o digito
            add = 0;   
            for (i=0; i < 9; i ++)      
                add += parseInt(cpf.charAt(i)) * (10 - i); 
                rev = 11 - (add % 11); 
                if (rev == 10 || rev == 11)    
                    rev = 0;   
                if (rev != parseInt(cpf.charAt(9)))    
                    return false;      
            // Valida 2o digito
            add = 0;   
            for (i = 0; i < 10; i ++)       
                add += parseInt(cpf.charAt(i)) * (11 - i); 
            rev = 11 - (add % 11); 
            if (rev == 10 || rev == 11)
                rev = 0;   
            if (rev != parseInt(cpf.charAt(10)))
                return false;      
            return true;  

        },
        refreshUserSales: function(){
            $.ajax({
                url: "/reimbursements/available/count",
                type: 'GET'
            })
            .done(function() {
            })
            .fail(function(response) {
            })
            .success(function(response) {
                if(response.total_qty > 0){
                    $("body #empty-user-sales").hide("fast");
                    $("body #user-sales").show("fast");
                    $("body #user-sales").attr('data-badge', response.total_qty);
                } else {
                    $("body #empty-user-sales").show("fast");
                    $("body #user-sales").hide("fast");
                }
            });
        },
        refreshShoppingCart: function(){
            if ($("body #shopping-cart").length == 0){
                return false;
            }
            $.ajax({
                url: "https://partyou.com.br/user-cart",
                type: 'GET'
            })
            .done(function() {
            })
            .fail(function(response) {
            })
            .success(function(response) {
                if(response.total_qty > 0){
                    $("body #empty-shopping-cart").hide("fast");
                    $("body #shopping-cart").show("fast");
                    $("body #shopping-cart").attr('data-badge', response.total_qty);
                } else {
                    $("body #empty-shopping-cart").show("fast");
                    $("body #shopping-cart").hide("fast");
                }
            });
        },
        checkUserEmail: function(email_user, cpf, initCb, doneCb, failCb, nUrl=null, csrf_token=null){
            if(initCb){
                initCb();
            }
            const data = {
                csrf_token: csrf_token,
                user_email: email_user,
                cpf: cpf
            };

            const url = (!nUrl) ? '/check-user-email' : nUrl;

            $.ajax({
                url: url,
                type: "POST",
                data: data
            })
            .done(function(){
                if(doneCb){
                    doneCb();
                }
            })
            .fail(function(xhr){
                const msg = xhr.responseJSON;
                if(msg.error !== undefined && failCb){
                    failCb(msg);
                }
            });          
        },
