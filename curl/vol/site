                    missingFields.push("<li>Utilize um email válido</li>");
                }
                if(!signupPassword.val()){
                    missingFields.push("<li>Campo <strong>senha</strong> é obrigatório</li>");
                }
                if(userAction == "signup" && !signupName.val()){
                    missingFields.push("<li><strong>Nome Completo</strong> é um campo obrigatório</li>");
                }
                if (userAction == "signup" && cpfDocument.length > 0 && (cpfDocument.val() == undefined || !userJS.validCPF(cpfDocument.val()))){
                    missingFields.push("<li><strong>CPF</strong> inválido</li>");
                }
                if(!recaptcha.val()){
                    missingFields.push("<li>Prove que não é um <strong>robô</strong></li>");
                }
                if(warningBoxJS.displayError(missingFields)){
                    return false;
                }
                warningDiv.slideUp("fast");
                eventJS.createPartnerEvent(userAction, url);
            },
